<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="dbTools.OrdersEntity" table="orders" schema="myshop_database">
        <id name="id" column="id"/>
        <property name="orderDate" column="OrderDate"/>
        <property name="userName" column="UserName"/>
        <property name="purchases" column="Purchases"/>
        <property name="addressee" column="Addressee"/>
        <property name="withCurier" column="WithCurier"/>
        <property name="shopId" column="ShopId"/>
    </class>
</hibernate-mapping>